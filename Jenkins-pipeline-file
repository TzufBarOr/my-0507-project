properties([pipelineTriggers([pollSCM('* * * * *')])])
def myBanner(){
    return '''*****
start
*****
'''
}
node {
    stage("clone"){
        git branch: 'main', url: 'https://github.com/TzufBarOr/my-Jenkins-pipeline-test.git'
            echo myBanner()
    }
    stage("show files"){
        bat "dir"
    }
    stage("show my IP")
        bat "python showing host name and IP.py"
    }
    stage("run python"){
        bat "python ${env.PythonScript}"
        }
    
}
