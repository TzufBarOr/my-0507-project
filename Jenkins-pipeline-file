properties([pipelineTriggers([pollSCM('* * * * *')])])
node {
    stage("clone"){
        git branch: 'main', url: 'https://github.com/TzufBarOr/my-0507-project.git'
    }
    stage("show files"){
        bat "dir"
    }
    stage("show my IP")
        bat "python showing host name and IP.py"
    }
    stage("run python"){
        bat "python ${env.PythonScript}"
        }
    
}
